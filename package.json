{
  "name": "cbgenerate",
  "version": "1.0.0",
  "description": "Couchbase Data Generator",
  "scripts": {
    "clean": "rm -rf ./dist",
    "compile": "npm run clean; babel app/ -d ./dist",
    "watch": "npm run clean; babel app/ -d ./dist --watch",
    "postinstall": "npm run compile"
  },
  "author": "Aaron Benton",
  "license": "MIT",
  "bin": {
    "queue": "bin/cbgenerate"
  },
  "private": true,
  "dependencies": {
    "chalk": "^1.1.1",
    "commander": "^2.9.0",
    "couchbase": "^2.1.5",
    "faker": "^3.1.0"
  },
  "devDependencies": {
    "eslint": "^2.8.0",
    "babel-cli": "^6.7.7",
    "babel-eslint": "^6.0.3",
    "babel-preset-es2015": "^6.6.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0"
  }
}
